<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
     
        <referenceContainer name="page.top">
            <container name="mycustom.div" htmlTag="div" htmlClass="inner-banner custom-breadcrumbs" />
        </referenceContainer>
           <move element="category.image" destination="mycustom.div" after="breadcrumbs"/>
        <move element="breadcrumbs" destination="mycustom.div" before="-"/>
         <!-- <referenceBlock name="category_view_custom_block" remove="true"/> -->
         <referenceContainer name="category.product.list.additional">
            <container name="action-wraper-test" htmlTag="div" htmlClass="action-wraper-test" />
        </referenceContainer>
        <referenceContainer name="quick_navigation.filter_list" remove="true"/>
    </body>
</page>
