<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" label="Customer My Account (All Pages)" design_abstraction="custom">
    <body>
        <referenceContainer name="page.wrapper">
             <container name="account.banner" htmlTag="div" htmlClass="inner-banner custom-breadcrumbs" before="main.content">
                <block class="Magento\Framework\View\Element\Template" name="custom.file" template="Magento_Customer::account_banner.phtml" />
            </container>
        </referenceContainer>
        <move element="page.main.title" destination="account.banner" before="-"/>


    </body>
</page>